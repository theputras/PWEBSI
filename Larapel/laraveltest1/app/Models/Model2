<?php
// week5
namespace App\Models;

class Model2
{
    public function PenampilanModel()
    {
        echo 'Hello model 2 laravel';
    }

    public function ProsedureModel($a, $b)
    {
        $c = $a - $b;
        echo 'Hasil :' . $c;
    }

    public function FungsinyaModel($a, $b)
    {
        $c = $a - $b;
        return $c;
    }
}
